             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:helpers="clr-namespace:StarkCNC.Helpers"
             xmlns:lang="clr-namespace:StarkCNC.Localization"
             xmlns:local="clr-namespace:StarkCNC.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <helpers:EmptyToVisibilityConverter x:Key="EmptyToVisibilityConverter"/>
    </UserControl.Resources>
    <Grid x:Name="FlyoutMenu"/>
</UserControl>
